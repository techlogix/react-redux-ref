{
  "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env":{
        "browser":true,
        "node": true,
        "jquery": true
    },
    "rules":{
        "quotes":0,
        "no-trailing-spaces":0,
        "eol-last":0,
        "no-unsaved-vars":0

    },
    "globals":{
        "jQuery": true,
        "$": true
    }
}